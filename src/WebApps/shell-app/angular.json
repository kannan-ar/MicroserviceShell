{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "shell-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/shell-app",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "customWebpackConfig": {
              "path": "webpack.config.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "shell-app:build:production",
              "customWebpackConfig": {
                "path": "webpack.config.ts"
              }
            },
            "development": {
              "browserTarget": "shell-app:build:development",
              "port": 7000
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "shell-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "utils": {
      "projectType": "library",
      "root": "projects/utils",
      "sourceRoot": "projects/utils/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/utils/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/utils/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/utils/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/utils/src/test.ts",
            "tsConfig": "projects/utils/tsconfig.spec.json",
            "karmaConfig": "projects/utils/karma.conf.js"
          }
        }
      }
    },
    "identity": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/identity",
      "sourceRoot": "projects/identity/src",
      "prefix": "pmo",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/identity",
            "index": "projects/identity/src/index.html",
            "main": "projects/identity/src/main.ts",
            "polyfills": "projects/identity/src/polyfills.ts",
            "tsConfig": "projects/identity/tsconfig.app.json",
            "assets": [
              "projects/identity/src/favicon.ico",
              "projects/identity/src/assets"
            ],
            "styles": [
              "projects/identity/src/styles.css"
            ],
            "scripts": [],
            "customWebpackConfig": {
              "path": "projects/identity/webpack.config.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/identity/src/environments/environment.ts",
                  "with": "projects/identity/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "identity:build:production",
              "customWebpackConfig": {
                "path": "projects/identity/webpack.config.ts"
              }
            },
            "development": {
              "browserTarget": "identity:build:development",
              "port": 7001
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "identity:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/identity/src/test.ts",
            "polyfills": "projects/identity/src/polyfills.ts",
            "tsConfig": "projects/identity/tsconfig.spec.json",
            "karmaConfig": "projects/identity/karma.conf.js",
            "assets": [
              "projects/identity/src/favicon.ico",
              "projects/identity/src/assets"
            ],
            "styles": [
              "projects/identity/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "shell-service": {
      "projectType": "library",
      "root": "projects/shell-service",
      "sourceRoot": "projects/shell-service/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/shell-service/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/shell-service/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/shell-service/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/shell-service/src/test.ts",
            "tsConfig": "projects/shell-service/tsconfig.spec.json",
            "karmaConfig": "projects/shell-service/karma.conf.js"
          }
        }
      }
    },
    "shell-models": {
      "projectType": "library",
      "root": "projects/shell-models",
      "sourceRoot": "projects/shell-models/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/shell-models/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/shell-models/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/shell-models/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/shell-models/src/test.ts",
            "tsConfig": "projects/shell-models/tsconfig.spec.json",
            "karmaConfig": "projects/shell-models/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "shell-app"
}
